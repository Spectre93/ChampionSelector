					<LinearLayout
						android:layout_width="fill_parent"
						android:layout_height="0.0dp"
						android:layout_weight="1.0"
						android:paddingBottom="5.0dp"
						android:paddingLeft="4.0dp"
						android:paddingRight="4.0dp">

						<ImageView
							android:id="@id/right_2"
							android:layout_width="116.0dp"
							android:layout_height="84.0dp"
							android:layout_gravity="center"
							android:background="@drawable/other_champframe"
							android:onClick="onClick"
							android:paddingBottom="14.0dp"
							android:paddingEnd="30.0dp"
							android:paddingStart="30.0dp"
							android:paddingTop="14.0dp"/>
					</LinearLayout>

					<LinearLayout
						android:layout_width="fill_parent"
						android:layout_height="0.0dp"
						android:layout_weight="1.0"
						android:paddingBottom="5.0dp"
						android:paddingLeft="4.0dp"
						android:paddingRight="4.0dp">

						<ImageView
							android:id="@id/right_3"
							android:layout_width="116.0dp"
							android:layout_height="84.0dp"
							android:layout_gravity="center"
							android:background="@drawable/other_champframe"
							android:onClick="onClick"
							android:paddingBottom="14.0dp"
							android:paddingEnd="30.0dp"
							android:paddingStart="30.0dp"
							android:paddingTop="14.0dp"/>
					</LinearLayout>

					<LinearLayout
						android:layout_width="fill_parent"
						android:layout_height="0.0dp"
						android:layout_weight="1.0"
						android:paddingBottom="5.0dp"
						android:paddingLeft="4.0dp"
						android:paddingRight="4.0dp">

						<ImageView
							android:id="@id/right_4"
							android:layout_width="116.0dp"
							android:layout_height="84.0dp"
							android:layout_gravity="center"
							android:background="@drawable/other_champframe"
							android:onClick="onClick"
							android:paddingBottom="14.0dp"
							android:paddingEnd="30.0dp"
							android:paddingStart="30.0dp"
							android:paddingTop="14.0dp"/>
					</LinearLayout>

					<LinearLayout
						android:layout_width="fill_parent"
						android:layout_height="0.0dp"
						android:layout_weight="1.0"
						android:paddingLeft="4.0dp"
						android:paddingRight="4.0dp">

						<ImageView
							android:id="@id/right_5"
							android:layout_width="116.0dp"
							android:layout_height="84.0dp"
							android:layout_gravity="center"
							android:background="@drawable/other_champframe"
							android:onClick="onClick"
							android:paddingBottom="14.0dp"
							android:paddingEnd="30.0dp"
							android:paddingStart="30.0dp"
							android:paddingTop="14.0dp"/>
					</LinearLayout>
				</LinearLayout>
			</ScrollView>
		</LinearLayout>
	</FrameLayout>
</LinearLayout>